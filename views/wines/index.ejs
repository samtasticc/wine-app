<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/stylesheets/style.css"/>
    <title>wines you've tried</title>
</head>
<body>
    <%- include('../partials/navbar.ejs') %>
    <a href="/users/<%=user._id%>/wines/new">Add a new wine.</a>
    <h1>Welcome to Sip Happens, <%= user.username %>!</h1>
    <h3>wines you've tried</h3>
    <ul>
        <% wines.forEach((wines)=>{ %>
            <a href="/users/<%= user._id %>/wines/<%= wines._id %>">
            <li><%= wines.brand %> - <%= wines.varietal %></li>
        <% }) %>    
            </a>
    </ul>
</body>
</html>